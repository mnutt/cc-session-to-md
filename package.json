  "main": "dist/index.js",
  "bin": {
    "cc-session-to-md": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit"
